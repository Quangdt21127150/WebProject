<form action="<%= submitPath %>?_csrf=<%= locals.csrfToken %>" method="POST">
  <p>
    <label for="title">Title</label>
    <input
      type="text"
      id="title"
      name="title"
      value="<%= category.title %>"
      required
    />
  </p>

  <p>
    <button type="reset" class="btn btn-alt">Reset</button>
    <button class="btn btn-solid">Save</button>
  </p>
</form>
